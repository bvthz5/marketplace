    "@svgr/webpack": "^6.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "babel-jest": "^29.4.2",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "msw": "^1.2.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0"
  },
