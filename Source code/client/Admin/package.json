{
  "name": "@minimal/material-kit-react",
  "author": "minimals.cc",
  "licence": "MIT",
  "version": "1.6.0",
  "private": false,
  "scripts": {
    "start:dev": "env-cmd -f .env.dev react-scripts start",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "start:qa": "env-cmd -f .env.test react-scripts start",
    "build:dev": "env-cmd -f ./.env.dev react-scripts build",
    "build:prod": "env-cmd -f ./.env.prod react-scripts build",
    "build:qa": "env-cmd -f ./.env.qa react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "test": "jest",
    "coverage": "jest --coverage",
    "sonar-scanner:dev": "sonar-scanner -Dproject.settings=./sonar-project-dev.properties",
    "sonar-scanner:prod": "sonar-scanner -Dproject.settings=./sonar-project-prod.properties"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
