    "react-places-autocomplete": "^7.3.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.1.1",
    "react-top-loading-bar": "^2.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "simplebar-react": "^2.4.3",
